{
  "name": "@tradle/serverless",
  "version": "1.0.0",
  "main": "index.js",
  "private": "true",
  "repository": "https://github.com/tradle/aws",
  "author": "Mark Vayngrib <mark@tradle.io>",
  "license": "MIT",
  "devDependencies": {
    "@tradle/aws-client": "github:tradle/aws-client",
    "@tradle/cb-blockr": "tradle/cb-blockr",
    "@tradle/engine": "^4.10.0",
    "@tradle/restore": "tradle/restore",
    "@tradle/test-helpers": "tradle/test-helpers",
    "array-shuffle": "^1.0.1",
    "aws-iot-device-sdk": "mvayngrib/aws-iot-device-sdk-js",
    "aws-sdk": "^2.60.0",
    "aws-sdk-mock": "^1.7.0",
    "aws-xray-sdk": "^1.1.3",
    "bindall": "^2.0.1",
    "commander": "^2.9.0",
    "deep-equal": "^1.0.1",
    "eslint": "^3.19.0",
    "flow-bin": "^0.48.0",
    "flow-remove-types": "^1.2.1",
    "isomorphic-fetch": "^2.2.1",
    "js-yaml": "^3.8.4",
    "leveldown": "^1.7.2",
    "levelup": "^1.3.8",
    "memdown": "^1.2.4",
    "mkdirp": "^0.5.1",
    "nkey-bitcoin": "^1.2.0",
    "nkey-curve25519": "^1.0.1",
    "nkey-ecdsa": "^1.1.0",
    "nkey-ethereum": "tradle/nkey-ethereum",
    "node-fetch": "^1.7.1",
    "pify": "^3.0.0",
    "random-name": "^0.1.2",
    "re-emitter": "^1.1.3",
    "react-native-debugger-open": "^0.3.11",
    "serverless": "mvayngrib/serverless",
    "serverless-apigw-binary": "^0.3.5",
    "serverless-plugin-tracing": "^1.0.6",
    "serverless-resources-env": "mvayngrib/serverless-resources-env",
    "serverless-sam": "mvayngrib/serverless-sam",
    "tape": "^4.6.3"
  },
  "scripts": {
    "setup": "cd project && npm i && cd .. && npm i",
    "gen:testenv": "./scripts/gen-env.js",
    "gen:fixtures": "./scripts/gen-fixtures.js",
    "lint": "eslint --quiet --ignore-path .gitignore .",
    "test": "npm run dynamo:start && NODE_ENV=test tap `git ls-files project/test/*.test.js`",
    "deploy:local": "npm run lint && npm run compile-template && SLS_DEBUG=* IS_LOCAL=1 serverless deploy",
    "deploy": "npm run compile-template && SLS_DEBUG=* IS_LOCAL= serverless deploy && npm run makepublic && npm run gen:testenv",
    "deploy:safe": "npm run lint && npm test && npm run rebuild && npm run deploy",
    "rebuild": "cd project && docker run --rm -v \"$PWD\":/var/task lambci/lambda:build-nodejs6.10 && cd ..",
    "rebuild:local": "cd project && npm rebuild && cd ..",
    "nuke": "./scripts/nuke.sh",
    "delete:typetables": "node ./scripts/delete-tables.js",
    "build": "flow-remove-types project/ -d build/ && npm run copynonjs",
    "copynonjs": "find project/lib project/conf project/test -type f '!' -name '*.js' | cpio -pdm build/",
    "export:template": "serverless sam export --output ./template.yml",
    "makepublic": "./scripts/makepublic.js",
    "compile-template": "./scripts/compile.js serverless-uncompiled.yml > serverless.yml",
    "reinstall": "rm -rf node_modules project/node_modules && npm run setup",
    "dynamo:start": "docker-compose -f ./project/test/docker-compose-dynamo.yml up -d",
    "dynamo:stop": "docker-compose -f ./project/test/docker-compose-dynamo.yml down",
    "tail": "./scripts/tail.sh"
  },
  "dependencies": {}
}
