  "author": "Mark Vayngrib <mark@tradle.io>",
  "license": "MIT",
  "devDependencies": {
    "@tradle/aws-client": "github:tradle/aws-client",
    "@types/aws-sdk": "^2.7.0",
    "@types/node": "^8.0.30",
    "aws-sdk": "^2.60.0",
    "aws-sdk-mock": "^1.7.0",
    "bindall": "^2.0.1",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^3.19.0",
    "husky": "^0.14.3",
    "is-native-module": "^1.1.0",
    "js-yaml": "^3.10.0",
    "lint-staged": "^4.2.3",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "nock": "^9.0.19",
    "node-env-file": "^0.1.8",
    "serverless": "github:mvayngrib/serverless",
    "serverless-apigw-binary": "^0.3.5",
    "serverless-dynamodb-autoscaling": "^0.6.2",
    "serverless-offline": "github:tradle/serverless-offline",
    "serverless-plugin-aws-alerts": "^1.2.4",
    "serverless-plugin-common-excludes": "^2.0.0",
    "serverless-plugin-tracing": "^1.0.6",
    "sinon": "^3.3.0",
    "tape": "^4.6.3",
    "validate-commit-msg": "^2.14.0"
  },
  "scripts": {
    "setup": "npm i && npm run build:yml && npm run package",
    "precommit": "lint-staged",
    "package": "serverless package",
    "getenv": "./scripts/gen-env.js",
    "gen:fixtures": "./scripts/gen-fixtures.js",
    "gen:samples": "./scripts/gen-samples.js",
    "gen:localsamples": "./scripts/gen-samples.js --local",
    "gen:localresources": "./scripts/gen-local-resources.js",
    "gen:resourcedefs": "./scripts/gen-resource-defs.js",
    "gen:modelmap": "./scripts/map-models-to-buckets.js",
    "reset:local": "npm run localstack:restart && sleep 4 && npm run gen:localresources",
    "eslint": "eslint --quiet --ignore-path .gitignore .",
    "test": "node test",
    "test:setup": "npm run localstack:start && npm run gen:localresources",
    "test:e2e": "BLUEBIRD_DEBUG=1 DEBUG=λ*,*tradle* node scripts/end-to-end-test.js",
    "test:e2e:debug": "BLUEBIRD_DEBUG=1 DEBUG=λ*,*tradle* node --debug --inspect scripts/end-to-end-test.js",
    "test:graphqlserver": "node ./scripts/graphql-server",
    "test:graphqlserver:debug": "node --debug --inspect ./scripts/graphql-server",
    "deploy:prepare": "npm run eslint && npm run build:yml && npm run gen:modelmap && npm run gen:resourcedefs",
    "deploy:local": "npm run deploy:prepare && SLS_DEBUG=* IS_LOCAL=1 ./scripts/deploy.js",
    "deploy:base": "SLS_DEBUG=* IS_LOCAL= ./scripts/deploy.js && npm run deploy:postprocess",
    "deploy": "npm run deploy:prepare && npm run deploy:base",
    "deploy:safe": "npm run deploy:prepare && npm test && npm run rebuild:lambda && npm run deploy:base",
    "deploy:fromscratch": "npm run reinstall && npm run localstack:start && npm run gen:localresources && npm run deploy:safe",
    "deploy:postprocess": "npm run makepublic && npm run getenv",
    "rebuild:lambda": "./scripts/rebuild-lambda.sh",
    "rebuild:local": "npm rebuild",
    "nuke": "./scripts/nuke.sh",
    "delete:typetables": "node ./scripts/delete-tables.js",
    "clear:tables": "DEBUG=* node ./scripts/clear-tables.js",
    "copynonjs": "./scripts/copy-non-compiled.js",
    "export:template": "serverless sam export --output ./template.yml",
    "makepublic": "./scripts/makepublic.js",
    "compile-template": "./scripts/compile.js serverless-uncompiled.yml serverless.yml",
    "interpolate-template": "sls print > serverless-tmp.yml && mv serverless-tmp.yml serverless.yml",
    "build:yml": "npm run compile-template && npm run interpolate-template && npm run gen:resourcedefs",
    "reinstall": "rm -rf node_modules && npm install",
    "localstack:start": "./scripts/localstack.sh up",
    "localstack:stop": "./scripts/localstack.sh down",
    "localstack:restart": "./scripts/localstack.sh down && ./scripts/localstack.sh up",
    "tail": "./scripts/tail.sh",
    "info": "serverless info",
    "commit": "git-cz",
    "tswatch": "tsc -w",
    "tslint": "tslint -p .",
    "symlink:dist": "ln -sf ../.serverless ./dist/.serverless && ln -sf ../conf ./dist/conf",
    "invoke:http": "./scripts/invoke-unzip.sh",
    "delete:applications": "./scripts/delete-applications.js",
    "getmyinfo": "./scripts/info.js > myinfo.json && open ./myinfo.json",
    "setstyle": "./scripts/setstyle.sh",
    "addfriends": "./scripts/addfriends.js",
    "reshrink": "npm dedupe && npm prune && npm shrinkwrap"
  },
  "dependencies": {
    "@meltwater/fetch-favicon": "^1.0.4",
    "@tradle/aws-client": "github:tradle/aws-client",
    "@tradle/bitcoin-adapter": "tradle/bitcoin-adapter",
    "@tradle/biz-plugins": "github:tradle/biz-plugins#cloud",
    "@tradle/bot-employee-manager": "github:tradle/bot-employee-manager#cloud",
    "@tradle/bot-products": "github:tradle/bot-products#modeled",
    "@tradle/build-resource": "^2.9.1",
    "@tradle/cb-blockr": "^3.2.1",
    "@tradle/constants": "^2.1.0",
    "@tradle/custom-models": "github:tradle/custom-models",
    "@tradle/dynamodb": "github:tradle/dynamodb",
    "@tradle/embed": "github:tradle/embed",
    "@tradle/engine": "^5.2.0",
    "@tradle/ethereum-adapter": "github:tradle/ethereum-adapter",
    "@tradle/gen-samples": "github:tradle/gen-samples",
    "@tradle/merge-models": "^2.1.0",
    "@tradle/models": "github:tradle/models",
    "@tradle/models-corporate-onboarding": "github:tradle/models-corporate-onboarding",
    "@tradle/schema-graphql": "github:tradle/schema-graphql",
    "@tradle/schema-joi": "github:tradle/schema-joi",
    "@tradle/test-helpers": "github:tradle/test-helpers",
    "@tradle/validate-model": "^2.4.2",
    "@tradle/validate-resource": "^2.11.1",
    "aws-xray-sdk": "^1.1.4",
    "bluebird": "^3.5.0",
    "bluebird-q": "^2.1.1",
    "bn.js": "^4.11.8",
    "body-parser": "^1.17.2",
    "clone": "^2.1.1",
    "co": "^4.6.0",
    "co-express": "^2.0.0",
    "compression": "^1.7.0",
    "cors": "^2.8.4",
    "data-uri-to-file": "^0.1.8",
    "datauri": "^1.0.5",
    "debug": "^2.6.8",
    "deep-equal": "^1.0.1",
    "deep-extend": "^0.5.0",
    "dynamodb-marshaler": "^2.0.0",
    "error-ex": "^1.3.1",
    "event-hooks": "^1.2.0",
    "express": "^4.15.4",
    "express-graphql": "^0.6.7",
    "graphql": "^0.10.5",
    "helmet": "^3.8.1",
    "is-generator-function": "^1.0.6",
    "json-stable-stringify": "^1.0.1",
    "json-stringify-safe": "^5.0.1",
    "key-encoder": "mvayngrib/key-encoder-js",
    "lru-cache": "^4.1.1",
    "microtime-nodejs": "^1.0.0",
    "mkdirp": "^0.5.1",
    "mockery": "^2.1.0",
    "nkey-bitcoin": "^1.2.0",
    "nkey-curve25519": "^1.0.1",
    "nkey-ecdsa": "^1.1.0",
    "nkey-ethereum": "github:tradle/nkey-ethereum",
    "node-fetch": "^1.7.3",
    "object.omit": "^2.0.1",
    "object.pick": "^1.2.0",
    "pify": "^2.3.0",
    "promise-locker": "^1.0.0",
    "random-name": "^0.1.2",
    "serverless-http": "github:mvayngrib/serverless-http",
    "settle-promise": "^1.0.0",
    "source-map-support": "^0.4.18",
    "string-format": "^0.5.0",
    "superagent": "^3.5.2",
    "typeforce": "^1.11.1",
    "uuid": "^3.1.0",
    "xtend": "^4.0.1",
    "ya-promise-queue": "^1.1.0",
    "yn": "^2.0.0"
  },
  "lint-staged": {
    "*.{ts}": [
      "prettier --write",
      "git add"
    ]
  }
}
