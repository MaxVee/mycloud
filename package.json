  "scripts": {
    "setup": "npm i && npm run build:slsyml && npm run package",
    "package": "serverless package",
    "gen:testenv": "./scripts/gen-env.js",
    "gen:fixtures": "./scripts/gen-fixtures.js",
    "gen:samples": "./scripts/gen-samples.js",
    "gen:localsamples": "./scripts/gen-samples.js --local",
    "gen:localresources": "./scripts/gen-local-resources.js",
    "reset:local": "npm run localstack:restart && sleep 4 && npm run gen:localresources",
    "eslint": "eslint --quiet --ignore-path .gitignore .",
    "test": "tap test/*.test.js",
    "test:e2e": "DEBUG=@tradle/bot*,tradle:sls:bot* node scripts/end-to-end-test.js",
    "test:e2e:debug": "DEBUG=@tradle/bot*,tradle:sls:bot* node --debug --inspect scripts/end-to-end-test.js",
    "test:graphqlserver": "node ./scripts/graphql-server",
    "deploy:local": "npm run tslint && SLS_DEBUG=* IS_LOCAL=1 ./scripts/deploy.js dev",
    "deploy": "SLS_DEBUG=* IS_LOCAL= ./scripts/deploy.js && npm run makepublic && npm run gen:testenv",
    "deploy:safe": "npm test && npm run rebuild:lambda && ./scripts/deploy.js dev",
    "rebuild:lambda": "docker run --rm -v \"$PWD\":/var/task lambci/lambda:build-nodejs6.10",
    "rebuild:local": "npm rebuild",
    "nuke": "./scripts/nuke.sh",
    "delete:typetables": "node ./scripts/delete-tables.js",
    "build": "flow-remove-types  -d build/ && npm run copynonjs",
    "copynonjs": "./scripts/copy-non-compiled.js",
    "export:template": "serverless sam export --output ./template.yml",
    "makepublic": "./scripts/makepublic.js",
    "compile-template": "./scripts/compile.js serverless-uncompiled.yml > serverless.yml",
    "interpolate-template": "sls print | ./scripts/postprocess-yml.js > serverless-tmp.yml && mv serverless-tmp.yml serverless.yml",
    "build:slsyml": "npm run compile-template && npm run interpolate-template",
    "reinstall": "rm -rf node_modules && npm run setup",
    "localstack:start": "./scripts/localstack.sh up",
    "localstack:stop": "./scripts/localstack.sh down",
    "localstack:restart": "./scripts/localstack.sh down && ./scripts/localstack.sh up",
    "tail": "./scripts/tail.sh",
    "info": "serverless info",
    "precommit": "npm run build:slsyml && git add serverless.yml",
    "commit": "git-cz",
    "watch": "tsc -w",
    "tslint": "tslint **/*.ts",
    "symlink:dist": "ln -sf ../.serverless ./dist/.serverless && ln -sf ../conf ./dist/conf"
  },
  "dependencies": {
    "@meltwater/fetch-favicon": "^1.0.4",
