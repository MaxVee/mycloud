  "scripts": {
    "init": "npm i && npm run build:yml",
    "precommit": "lint-staged",
    "package": "serverless package",
    "gen:testenv": "./lib/scripts/gen-test-env.js",
    "gen:fixtures": "./lib/scripts/gen-fixtures.js",
    "gen:samples": "./lib/scripts/gen-samples.js",
    "gen:localsamples": "./lib/scripts/gen-samples.js --local",
    "gen:localresources": "./lib/scripts/gen-local-resources.js",
    "gen:resourcedefs": "./lib/scripts/gen-resource-defs.js",
    "gen:modelmap": "./lib/scripts/map-models-to-buckets.js",
    "reset:local": "npm run localstack:restart && sleep 6 && npm run gen:localresources",
    "eslint": "eslint --quiet --ignore-path .gitignore .",
    "setup:local": "npm run localstack:start && npm run gen:localresources",
    "pretest": "npm run copy-static-assets",
    "test": "node lib/test",
    "test:e2e": "BLUEBIRD_DEBUG=1 DEBUG=λ*,*tradle* node lib/scripts/end-to-end-test.js",
    "test:e2e:debug": "BLUEBIRD_DEBUG=1 DEBUG=λ*,*tradle* node --debug --inspect lib/scripts/end-to-end-test.js",
    "test:graphqlserver": "NODE_ENV=test node ./lib/scripts/graphql-server",
    "test:graphqlserver:debug": "NODE_ENV=test node --debug --inspect ./lib/scripts/graphql-server",
    "remote:graphqlserver": "node ./lib/scripts/graphql-server",
    "remote:graphqlserver:debug": "node --debug --inspect ./lib/scripts/graphql-server",
    "predeploy": "npm run copy-static-assets",
    "deploy:prepare": "npm run eslint && npm run build:yml",
    "deploy:local": "npm run deploy:prepare && SLS_DEBUG=* IS_LOCAL=1 ./lib/scripts/deploy.js",
    "deploy:base": "SLS_DEBUG=* IS_LOCAL= ./lib/scripts/deploy.js && npm run deploy:postprocess",
    "deploy": "npm run deploy:prepare && npm run deploy:base",
    "deploy:safe": "npm run deploy:prepare && npm run gen:localresources && npm test && npm run rebuild:lambda && npm run deploy:base",
    "deploy:fromscratch": "npm run reinstall && npm run localstack:start && npm run deploy:safe",
    "deploy:postprocess": "npm run gen:testenv",
    "rebuild:lambda": "./src/scripts/rebuild-lambda.sh",
    "rebuild:local": "npm rebuild",
    "nuke": "./src/scripts/nuke.sh",
    "delete:typetables": "node ./lib/scripts/delete-tables.js",
    "clear:tables": "DEBUG=* node ./lib/scripts/clear-tables.js",
    "copy-static-assets": "./src/scripts/copy-static-assets.sh",
    "export:template": "serverless sam export --output ./template.yml",
    "makepublic": "./lib/scripts/makepublic.js",
    "compile-template": "rm -f serverless.yml && ./lib/scripts/compile.js -i serverless-uncompiled.yml -o serverless.yml && cp serverless.yml serverless-compiled.yml",
    "interpolate-template": "./src/scripts/interpolate-template.sh",
    "build:yml": "npm run compile-template && npm run interpolate-template && npm run gen:resourcedefs",
    "reinstall": "rm -rf node_modules && npm install",
    "localstack:start": "./src/scripts/localstack.sh up",
    "localstack:stop": "./src/scripts/localstack.sh down",
    "localstack:restart": "./src/scripts/localstack.sh down && ./src/scripts/localstack.sh up",
    "redis:stop": "redis-cli shutdown",
    "tail": "./src/scripts/tail.sh",
    "info": "serverless info",
    "commit": "git-cz",
    "tswatch": "tsc -w",
    "tslint": "tslint -p .",
    "invoke:http": "./src/scripts/invoke-unzip.sh",
    "delete:applications": "./lib/scripts/delete-applications.js",
    "getmyinfo": "./lib/scripts/info.js > myinfo.json && open ./myinfo.json",
    "setenv": "npm run build:yml && ./lib/scripts/setenv.js",
    "setstyle": "./src/scripts/setstyle.sh",
    "setconf": "./src/scripts/setconf.sh",
    "setstyle:local": "IS_LOCAL=1 ./src/scripts/setstyle.sh",
    "setconf:local": "IS_LOCAL=1 ./src/scripts/setconf.sh",
    "addfriends": "./lib/scripts/addfriends.js",
    "reshrink": "npm dedupe && npm prune && npm shrinkwrap",
    "start": "./src/scripts/start.sh",
    "warmup": "serverless warmup",
    "postinstall": "./src/scripts/postinstall.sh",
    "remove": "npm run build:yml && serverless remove"
  },
  "dependencies": {
    "@meltwater/fetch-favicon": "^1.0.4",
    "@tradle/aws-client": "github:tradle/aws-client",
    "@tradle/bitcoin-adapter": "tradle/bitcoin-adapter",
    "@tradle/biz-plugins": "github:tradle/biz-plugins#cloud",
